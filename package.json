{
  "name": "markdown-components",
  "version": "2.0.1",
  "description": "Add user-safe, React-like components to your favorite markdown engine",
  "main": "lib/markdown-components.js",
  "browser": "lib/markdown-components.min.js",
  "repository": "https://github.com/aneilbaboo/markdown-components",
  "author": "Aneil Mallavarapu",
  "license": "MIT",
  "scripts": {
    "build": "WEBPACK_ENV=build webpack",
    "debugTest": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "dev": "WEBPACK_ENV=dev webpack --progress --colors --watch",
    "prepare": "yarn run build",
    "lint": "eslint src/**/*.js test/**/*.js",
    "test": "cross-env NODE_ENV=test jest",
    "test-with-coverage": "cross-env NODE_ENV=test jest --collectCoverage",
    "watch": "cross-env NODE_ENV=test jest --watch"
  },
  "dependencies": {
    "memory-streams": "^0.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^22.4.0",
    "jest-html-reporter": "^2.0.0",
    "jest-plugin-context": "^2.7.0",
    "jest-tobetype": "^1.1.0",
    "lodash": "^4.17.4",
    "markdown": "^0.5.0",
    "markdown-it": "^8.4.0",
    "markdown-js": "^0.0.3",
    "showdown": "^1.8.6",
    "uglify-js": "https://github.com/mishoo/UglifyJS2.git#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0"
  },
  "jest": {
    "setupFiles": [
      "jest-plugin-context/setup"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "json"
    ]
  }
}
